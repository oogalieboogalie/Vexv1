use "../src/vexc/tokenize.vex"
use "../src/vexc/program.vex"
use "../src/vexc/eval.vex"

fn main() {
    print("[vexc] running examples/index_assign.vex\n")
    let src = read_file("examples/index_assign.vex")
    let toks = vexc_tokenize(src)
    let prog = vexc_parse_program(toks, 0)
    let v = vexc_eval_program(prog)
    print("\n[vexc] returned {v}\n")
}
